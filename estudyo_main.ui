   <string notr="true">QMainWindow {
    background-color: #f5f5f5;
}

/* Sidebar Styling */
QFrame#sidebar {
    background-color: #2c5f7f;
    border: none;
}

QLabel#logoLabel {
    background-color: white;
    border-radius: 8px;
    font-size: 24px;
    color: #2c5f7f;
}

QLabel#appNameLabel {
    color: white;
    font-size: 24px;
    font-weight: bold;
}

/* Navigation Buttons */
QPushButton#navButton {
    background-color: transparent;
    color: white;
    border: none;
    border-left: 4px solid transparent;
    text-align: left;
    padding: 16px 24px 16px 48px;
    font-size: 15px;
}

QPushButton#navButton:hover {
    background-color: rgba(255, 255, 255, 0.1);
}

QPushButton#navButton:checked {
    background-color: rgba(255, 255, 255, 0.15);
    border-left: 4px solid #4CAF50;
    font-weight: 600;
}

/* Header Styling */
QFrame#headerFrame {
    background-color: white;
    border-bottom: 1px solid #e0e7ff;
}

QLabel#headerTitle {
    color: #2c5f7f;
    font-size: 24px;
    font-weight: bold;
}

/* Content Area */
QFrame#contentFrame {
    background-color: white;
    border-radius: 8px;
    border: 1px solid #e0e0e0;
}

/* Form Sections */
QGroupBox {
    background-color: white;
    border: 1px solid #bbdefb;
    border-radius: 8px;
    margin-top: 12px;
    padding-top: 12px;
    font-weight: bold;
    color: #2c5f7f;
    font-size: 16px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 4px 12px;
    color: #2c5f7f;
}

/* Input Fields */
QLineEdit, QComboBox, QSpinBox {
    padding: 10px;
    border: 1px solid #ddd;
    border-radius: 6px;
    background-color: white;
    font-size: 14px;
}

QLineEdit:focus, QComboBox:focus, QSpinBox:focus {
    border: 1px solid #2196F3;
}

QComboBox::drop-down {
    border: none;
    padding-right: 10px;
}

/* Labels */
QLabel {
    color: #333;
    font-weight: 600;
}

/* Buttons */
QPushButton {
    padding: 10px 20px;
    border: none;
    border-radius: 6px;
    font-weight: 600;
    font-size: 14px;
}

QPushButton#btnAdd, QPushButton#btnSave {
    background-color: #4CAF50;
    color: white;
}

QPushButton#btnAdd:hover, QPushButton#btnSave:hover {
    background-color: #45a049;
}

QPushButton#btnEdit, QPushButton#btnSearch, QPushButton#btnSort {
    background-color: #2196F3;
    color: white;
}

QPushButton#btnEdit:hover, QPushButton#btnSearch:hover, QPushButton#btnSort:hover {
    background-color: #1976D2;
}

QPushButton#btnDelete, QPushButton#btnClear {
    background-color: #f44336;
    color: white;
}

QPushButton#btnDelete:hover, QPushButton#btnClear:hover {
    background-color: #da190b;
}

QPushButton:disabled {
    background-color: #cccccc;
    color: #666666;
}

/* Table Styling */
QTableWidget {
    background-color: white;
    border: 1px solid #dee2e6;
    border-radius: 8px;
    gridline-color: #dee2e6;
}

QTableWidget::item {
    padding: 8px;
    border-bottom: 1px solid #dee2e6;
}

QTableWidget::item:selected {
    background-color: #e3f2fd;
    color: #000;
}

QTableWidget::item:hover {
    background-color: #f8f9fa;
}

QHeaderView::section {
    background-color: #f8f9fa;
    padding: 12px;
    border: none;
    border-bottom: 2px solid #dee2e6;
    font-weight: 600;
    color: #495057;
}

/* ScrollBar */
QScrollBar:vertical {
    border: none;
    background: #f0f0f0;
    width: 10px;
    margin: 0;
}

QScrollBar::handle:vertical {
    background: #c0c0c0;
    min-height: 20px;
    border-radius: 5px;
}

QScrollBar::handle:vertical:hover {
    background: #a0a0a0;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0;
}
